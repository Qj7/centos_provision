#!/usr/bin/env powscript


is_exists_file(file)
  debug "Checking ${file} file existence"
  if isset $SKIP_CHECKS
    debug "SKIP: Ð°ctual check of ${file} file existence disabled"
    return 0
  if [ -f "${file}" ]
    debug "OK: ${file} file exists"
    return 0
  else
    debug "NOK: ${file} file does not exist"
    return 1
